<!DOCTYPE html>
<html lang="en" style="background-color:white">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>MTM_Warehouse</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/login.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">
    @* <link rel="stylesheet" href="~/MTM_Warehouse.styles.css" asp-append-version="true" /> *@
</head>
<body>
        
    <div class="container-fluid">
        <main role="main" class="p-0" >
            @RenderBody()
        </main>
    </div> 
    @* @RenderSection("Scripts", required: false) *@

    <footer class="border-top footer text-muted bg-dark text-white mt-5" >
        <div class="container">
            <p class="">&copy; 2024 - MTM_Warehouse - <a asp-controller="Home" asp-action="Privacy">Privacy</a> </p>            
        </div>
    </footer>
   
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    @* <script src="~/lib/Chart.js/chart.js"></script> *@
    <script src="~/js/updateRoles.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
